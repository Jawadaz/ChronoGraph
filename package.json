{
  "name": "tauri",
  "dev:debug": "set NODE_OPTIONS=--inspect && npm run dev",
  "tauri:dev": "concurrently \"npm run dev\" \"tauri dev\" --kill-others-on-fail",
  "tauri:dev-debug": "concurrently \"npm run dev:debug\" \"tauri dev\" --kill-others-on-fail",
  "tauri:build": "tauri build",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dev:debug": "set NODE_OPTIONS=--inspect && vite",
    "tauri": "tauri",
    "tauri:dev": "concurrently \"npm run dev\" \"tauri dev\" --kill-others-on-fail",
    "tauri:dev-debug": "concurrently \"npm run dev:debug\" \"tauri dev\" --kill-others-on-fail",
    "tauri:build": "tauri build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.1",
    "happy-dom": "^19.0.2",
    "jsdom": "^27.0.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/cli": "^2.8.4",
    "@tauri-apps/plugin-dialog": "^2.4.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "cytoscape": "^3.33.1",
    "cytoscape-dagre": "^2.5.0",
    "cytoscape-fcose": "^2.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.5"
  },
  "optionalDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.50.1"
  }
