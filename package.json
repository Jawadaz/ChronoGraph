{
  "name": "tauri",
  "dev:debug": "set NODE_OPTIONS=--inspect && npm run dev",
  "tauri:dev": "concurrently \"npm run dev\" \"tauri dev\" --kill-others-on-fail",
  "tauri:dev-debug": "concurrently \"npm run dev:debug\" \"tauri dev\" --kill-others-on-fail",
  "tauri:build": "tauri build",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dev:debug": "set NODE_OPTIONS=--inspect && vite",
    "test": "jest",
    "test:ui": "playwright test",
    "test:ui:headed": "playwright test --headed",
    "test:ui:debug": "playwright test --debug",
    "test:ui:web": "playwright test --project=chromium-web",
    "test:ui:desktop": "playwright test --project=desktop-tauri",
    "test:ui:watch": "playwright test --watch",
    "test:ui:report": "playwright show-report",
    "tauri": "tauri",
    "tauri:dev": "concurrently \"vite --port 1429\" \"tauri dev\" --kill-others-on-fail",
    "tauri:dev-debug": "concurrently \"npm run dev:debug\" \"tauri dev\" --kill-others-on-fail",
    "tauri:build": "tauri build"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/cytoscape": "^3.21.9",
    "@types/jest": "^30.0.0",
    "concurrently": "^9.2.1",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1"
  },
  "dependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.50.1",
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/cli": "^2.8.4",
    "@tauri-apps/cli-win32-x64-msvc": "^2.8.4",
    "@types/d3": "^7.4.3",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "cytoscape": "^3.33.1",
    "cytoscape-dagre": "^2.5.0",
    "cytoscape-fcose": "^2.2.0",
    "d3": "^7.9.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.5"
  }
}
